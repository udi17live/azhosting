<div class="container">
    <div class="row align-items-center">
        <div class="col-sm-12 col-lg-6">
            <div class="header-page-content text-center text-lg-start">
                <h1>Domain names & pricing</h1>
                <p>Choose from over 400+ Top Level Domains at affordable prices.</p>
                <ul class="section-button">
                    <li><a href="#get-domain"><button class="btn btn-pill">Get your domain</button></a></li>
                    <li><a href="#pricing"><button class="btn btn-blue btn-pill">View Pricing</button></a></li>
                </ul>
            </div>
        </div>
    </div>
</div>
</header>


<section id="get-domain" 
class="domain-search-section-two default-box-shadow max-1000 bg-white margin-minus-box pt-100 pb-80 border-radius-3">
<div class="container">
    <div class="section-title section-title-two text-center">
        <h2>Search perfect domain</h2>
    </div>
    <div class="domain-search domain-search-two">
        <form  action="<%= process.env.RESELLER_HOSTING_URL %>/bill/index.php" method="POST">
            <div class="form-group">
                <input type="text" name="domain" class="form-control" placeholder="Find Your Domain" required>
                <div class="input-group-append">
                    <!-- <select class="form-control">
                        <option value="1">.com</option>
                        <option value="2">.co</option>
                        <option value="3">.net</option>
                    </select> -->
                </div>
                <button class="btn btn-gradient">Search Now</button>
            </div>
        </form>
        <div class="domain-search-category">
            <ul>
                <% selected_domains.forEach((selected_domain, index) => { %>
                    <li>
                        <a href="#" class="<%= index != 0 ? '': 'active' %>">.<%= selected_domain.tld %>  only $<%= selected_domain.final_cost %></a>
                    </li>
                <% }); %> 
            </ul>
        </div>
    </div>
</div>
</section>

<section class="choose-section pt-100 pb-70 blue-gradient-with-opacity">
    <div class="container">
        <div class="section-title section-title-two">
            <small>AZPIRE DOMAINS</small>
            <h2>Why choose us?</h2>
