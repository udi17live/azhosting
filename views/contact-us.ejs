<div class="mt-auto header-top-height"></div>
        <main class="container mb-auto">
            <div class="row">
                <div class="col-md-5">
                    <h3 class="mt-3 main-header-text-title mr-tp-60"><span>Still not sure what to choose? ?</span>We are here to help you.
                        
                    </h3>
                    <div class="contact-us-social-icons">
                        <a class="fb" href="#"><i class="fab fa-facebook-f"></i></a>
                        <a class="tw" href="#"><i class="fab fa-twitter"></i></a>
                        <a class="yb" href="#"><i class="fab fa-youtube"></i></a>
                    </div>
                </div>
                <div class="col-md-7">
                    <form class="row form-contain-home contact-page-form-send" id="ajax-contact" method="post"
                        action="/contact-us">

                        <h5>get in touch <span>discouver how can our services grow your business.</span></h5>
                        <div id="form-messages"></div>

                        <div class="col-md-12">
                            <%- include ("./partials/messages"); %>
                        </div>

                        <div class="col-md-12">

                            <div class="field input-field">
                                <input class="form-contain-home-input" type="text" id="name" name="name"
                                    placeholder="enter your name" required>

                                <i class="fas fa-user"></i>
                            </div>
                        </div>

                        <div class="col-md-12">

                            <div class="field input-field">
                                <input class="form-contain-home-input" type="email" id="email" name="email"
                                    placeholder="enter your email" required>

                                <i class="far fa-envelope"></i>
                            </div>
                        </div>

                        <div class="col-md-12">

                            <div class="field input-field">
                                <textarea class="form-contain-home-input" id="message" name="message"
                                    placeholder="enter your message" required></textarea>

                            </div>
                        </div>

                        <div class="btn-holder-contect">
                            <button type="submit">Send</button>

                        </div>
                    </form>

                </div>
            </div>
        </main>
        <div class="mt-auto"></div>
    </div>
    <div id="mobile-form-contact-height"></div>
    <section class="padding-20-0 mob-display-none">
        <div class="container">
            <div class="row justify-content-start">
                <div class="col-md-5">
                    <h5 class="immediate-help-center-title">Need further <br>assistance ?</h5>
                    <p class="immediate-help-center-text">If you are an existing customer you can use the below channels to recieve support.</p>
                    <a class="immediate-help-center-link" href="<%= process.env.RESELLER_PANEL_URL + '/support' %>" target="_blank">Open a support ticket</a>
                    <a class="immediate-help-center-link" href="#"><span class="mailto:hello@azpirehosting.info">hello@azpirehosting.info</span></a>
                </div>
            </div>
        </div>
    </section>